{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
            "serviceId": "website-react-app",
            "region": "europe-west2"
        }
      }
    ]
  }
}
